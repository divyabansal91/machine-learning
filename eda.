      "source": [
        "from textblob import TextBlob"
      ],
      "metadata": {
        "id": "zSqND_M0_239"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def textblob_sentiment_analysis(reviews):\n",
        "  sentiment = TextBlob(reviews).sentiment\n",
        "  if sentiment.polarity>0.1:\n",
        "   return 'positive'\n",
        "  elif sentiment.polarity<-0.1:\n",
        "   return'negative'\n",
        "  else:\n",
        "    return 'neutral'"
      ],
      "metadata": {
        "id": "h_Kr8ozZ74v2"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df['sentiment']=df['Review'].apply(textblob_sentiment_analysis)"
      ],
      "metadata": {
        "id": "wh5oEv0R9Lms"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
