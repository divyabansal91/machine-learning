    {
      "cell_type": "code",
      "source": [
        "# the function transformer is a tool in scikit-learn,\n",
        "  # a popular python library for machine learning,\n",
        " # that allows you to apply a specified function to the input data .\n",
        " #  the function transformer can be useful for\n",
        "  # performing custom transformations of input data in a machine learning pipeline."
      ],
      "metadata": {
        "id": "IUj2-gBR5t0d"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.preprocessing import FunctionTransformer\n",
        "import numpy as np\n",
        "\n",
        "# create a dataset\n",
        "x = np.array([[1, 2], [3, 4]])\n",
        "\n",
        "# define the transformation function.\n",
        "log_transformer = FunctionTransformer(np.log1p)\n",
        "\n",
        "# apply the transformation to the dataset.\n",
        "x_transformer= log_transformer.transform(x)\n",
        "\n",
        "# print the transformed dataset.\n",
        "print(x_transformer)"
      ],
      "metadata": {
